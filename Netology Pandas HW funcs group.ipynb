    "def find(pattern, path): # match a pattern\n",
    "    result = []\n",
    "    for root, dirs, files in os.walk(path):\n",
    "        for name in files:\n",
    "            if fnmatch.fnmatch(name, pattern):\n",
    "                print(f'{root}\\{name}')                \n",
    "                result.append(os.path.join(root, name))\n",
